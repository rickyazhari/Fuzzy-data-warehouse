    <UserControl.Resources>
        <!--fakta Pemesanan resource-->
        <!--fakta Enter-->
        <Storyboard x:Key="fakta_pemesanan_enter">
            <DoubleAnimation Storyboard.TargetName="fakta_pemesanan_span" Storyboard.TargetProperty="Width" To="15" BeginTime="0:0:0:0" Duration="0:0:0:0.1">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Storyboard.TargetName="fakta_pemesanan_border_split_right1" Storyboard.TargetProperty="Height" To="20" BeginTime="0:0:0:0.1" Duration="0:0:0:0.1">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Storyboard.TargetName="fakta_pemesanan_border_split_right2" Storyboard.TargetProperty="Height" To="20" BeginTime="0:0:0:0.1" Duration="0:0:0:0.1">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Storyboard.TargetName="fakta_pemesanan_border_top" Storyboard.TargetProperty="Width" To="280" BeginTime="0:0:0:0.2" Duration="0:0:0:0.2">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Storyboard.TargetName="fakta_pemesanan_border_bottom" Storyboard.TargetProperty="Width" To="280" BeginTime="0:0:0:0.2" Duration="0:0:0:0.2">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Storyboard.TargetName="fakta_pemesanan_border_split_left1" Storyboard.TargetProperty="Height" To="20" BeginTime="0:0:0:0.4" Duration="0:0:0:0.1">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Storyboard.TargetName="fakta_pemesanan_border_split_left2" Storyboard.TargetProperty="Height" To="20" BeginTime="0:0:0:0.4" Duration="0:0:0:0.1">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <ColorAnimation Storyboard.TargetName="fakta_pemesanan_inner" Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)" To="#FFFFFFFF" BeginTime="0:0:0:0.5" Duration="0:0:0:0.2">
                <ColorAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseInOut"></QuadraticEase>
                </ColorAnimation.EasingFunction>
            </ColorAnimation>
            <ColorAnimation Storyboard.TargetName="fakta_pemesanan_content" Storyboard.TargetProperty="(TextBlock.Background).(SolidColorBrush.Color)" To="#FFFFFFFF" BeginTime="0:0:0:0.5" Duration="0:0:0:0.2">
                <ColorAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseInOut"></QuadraticEase>
                </ColorAnimation.EasingFunction>
            </ColorAnimation>
            <ColorAnimation Storyboard.TargetName="fakta_pemesanan_content" Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" To="#FF008BFF" BeginTime="0:0:0:0.5" Duration="0:0:0:0.2">
                <ColorAnimation.EasingFunction>
