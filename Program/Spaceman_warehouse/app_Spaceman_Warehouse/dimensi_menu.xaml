    <!--User Control Resource-->
    <UserControl.Resources>
      <!--Dimensi WAktu resource-->
      <!--Dimensi Enter-->
        <Storyboard x:Key="dim_waktu_enter">
            <DoubleAnimation Storyboard.TargetName="dim_waktu_span" Storyboard.TargetProperty="Width" To="15" BeginTime="0:0:0:0" Duration="0:0:0:0.1">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Storyboard.TargetName="dim_waktu_border_split_right1" Storyboard.TargetProperty="Height" To="20" BeginTime="0:0:0:0.1" Duration="0:0:0:0.1">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Storyboard.TargetName="dim_waktu_border_split_right2" Storyboard.TargetProperty="Height" To="20" BeginTime="0:0:0:0.1" Duration="0:0:0:0.1">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Storyboard.TargetName="dim_waktu_border_top" Storyboard.TargetProperty="Width" To="280" BeginTime="0:0:0:0.2" Duration="0:0:0:0.2">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Storyboard.TargetName="dim_waktu_border_bottom" Storyboard.TargetProperty="Width" To="280" BeginTime="0:0:0:0.2" Duration="0:0:0:0.2">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Storyboard.TargetName="dim_waktu_border_split_left1" Storyboard.TargetProperty="Height" To="20" BeginTime="0:0:0:0.4" Duration="0:0:0:0.1">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Storyboard.TargetName="dim_waktu_border_split_left2" Storyboard.TargetProperty="Height" To="20" BeginTime="0:0:0:0.4" Duration="0:0:0:0.1">
                <DoubleAnimation.EasingFunction>
                    <QuadraticEase EasingMode="EaseIn"></QuadraticEase>
                </DoubleAnimation.EasingFunction>
