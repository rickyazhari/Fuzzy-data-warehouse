<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/charts" x:Class="app_Spaceman_Warehouse.chart_pop_produk" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">

    <dxc:ChartControl x:Name="bar_chart" HorizontalAlignment="Left" VerticalAlignment="Top">
        <dxc:ChartControl.Legend>
            <dxc:Legend/>
        </dxc:ChartControl.Legend>
        <dxc:XYDiagram3D Width="129">
            <dxc:BarSideBySideSeries3D DisplayName="kurang populer"/>
            <dxc:BarSideBySideSeries3D DisplayName="populer"/>
            <dxc:BarSideBySideSeries3D DisplayName="sangatpopuler"/>
        </dxc:XYDiagram3D>
    </dxc:ChartControl>

</UserControl>
